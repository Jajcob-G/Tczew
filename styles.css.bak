body {
	font-family: monaco;
	margin: 1;
	padding: 0;
	background-color: #3e2a35;
	color: #e26d5a;
}

header {
	background-color: #3e2a35;
	text-align: center;
	color: #e26d5a;
	padding: 20px;
	border: 5px solid #e26d5a;
}

p {
	background-color: #d7bba8;
	text-align: left;
	color: #3e2a35;
	padding: 20px;
	border: 5px solid #e26d5a;
}

ol {
	background-color: #bbb09b;
	text-align: left;
	color: #3e2a35;
	padding: 20px;
	border: 5px solid #e26d5a;
}

figure {
	display: block;
	margin-left: auto;
	margin-right:auto;
	width: 50%;
}

table {
	background-color: #bbb09b;
	text-align: left;
	color: #3e2a35;
	padding: 0px;
	border: 5px solid #e26d5a;
}